# Get the version of "Dell ControlVault w/o Fingerprint Sensor"
$driver = Get-WmiObject -Class Win32_PnpSignedDriver |
    Where-Object { $_.DeviceName -like "Dell ControlVault w/o Fingerprint Sensor*" }

if ($driver) {
    Write-Output "Driver Name : $($driver.DeviceName)"
    Write-Output "Driver Version : $($driver.DriverVersion)"
    Write-Output "Driver Provider : $($driver.DriverProviderName)"
} else {
    Write-Output "Dell ControlVault w/o Fingerprint Sensor driver not found."
}
